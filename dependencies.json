{
    "jquery": {
        "src": [
            "./node_modules/jquery/dist/jquery.min.js",
            "./node_modules/jquery/dist/jquery.min.map"
        ],
        "dest": "assets/vendor/jquery/",
        "js_link": "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.6.0/jquery.min.js"
    },
    "bootstrap": {
        "src": "./node_modules/bootstrap/dist/**/*",
        "dest": "assets/vendor/bootstrap/",
        "css_link": "https://cdnjs.cloudflare.com/ajax/libs/bootstrap/4.6.1/css/bootstrap.min.css",
        "js_link": "https://cdnjs.cloudflare.com/ajax/libs/bootstrap/4.6.1/js/bootstrap.bundle.min.js"
    },
    "jquery.easing": {
        "src": "./node_modules/jquery.easing/jquery.easing.min.js",
        "dest": "assets/vendor/jquery.easing/",
        "js_link": "https://cdnjs.cloudflare.com/ajax/libs/jquery-easing/1.4.1/jquery.easing.min.js"
    },
    "owl-carousel": {
        "src": "./node_modules/owl.carousel/dist/**/*",
        "dest": "assets/vendor/owl-carousel/",
        "css_link": "/assets/vendor/owl-carousel/assets/owl.carousel.min.css",
        "js_link": "/assets/vendor/owl-carousel/owl.carousel.min.js"
    },
    "iconify": {
        "js_link": "https://code.iconify.design/2/2.1.1/iconify.min.js"
