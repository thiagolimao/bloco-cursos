            {
                "src": "./node_modules/font-awesome/fonts/*",
                "dest": "assets/vendor/font-awesome/fonts/"
            }
        ],
        "css_link": "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css"
    },
    "aos": {
        "src": "./node_modules/aos/dist/**/*",
        "dest": "assets/vendor/aos/",
        "css_link": "https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css",
        "js_link": "https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.js"
